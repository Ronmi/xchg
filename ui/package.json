{
  "name": "ui",
  "version": "1.0.0",
  "description": "ui for xchg",
  "main": "js/bundle.js",
  "scripts": {
    "postinstall": "typings install",
    "typings": "typings",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "webpack": "webpack",
    "webpack:w": "webpack -w",
    "webpack:prod": "webpack --config webpack.minify.js",
    "lite": "browser-sync start --config bs-config.js",
    "start": "tsc && webpack && concurrently \"npm run tsc:w\" \"npm run webpack:w\" \"npm run lite\"",
    "karma": "karma start --browsers PhantomJS,Firefox,Chrome",
    "coverage": "mkdir -p coverage;touch coverage/coverage-final.json;rm -fr build;tsc && karma start coverage.karma.js",
    "travis": "npm run coverage",
    "test": "rm -fr build && tsc && find build/test -name '*.js' -type f | xargs mocha -r es6-shim mocha.bootstrap.js"
  },
  "author": "Ronmi Ren <ronmi.ren@gmail.com>",
  "license": "GPL-3.0",
  "dependencies": {
    "babel-polyfill": "^6.9.1",
    "react": "^15.2.1",
    "react-dom": "^15.2.1",
    "whatwg-fetch": "^1.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.11.4",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "chai": "^3.5.0",
    "chai-as-promised": "^5.3.0",
    "concurrently": "^2.2.0",
    "css-loader": "^0.23.1",
    "enzyme": "^2.4.1",
    "file-loader": "^0.9.0",
    "inline-source-map": "^0.6.2",
    "istanbul-instrumenter-loader": "^0.2.0",
    "jsdom": "^9.4.1",
    "json-loader": "^0.5.4",
    "karma": "^1.1.1",
    "karma-chai-as-promised": "^0.1.2",
    "karma-chai-sinon": "^0.1.5",
    "karma-chrome-launcher": "^1.0.1",
    "karma-cli": "^1.0.1",
    "karma-coverage": "^1.1.1",
    "karma-es5-shim": "0.0.4",
    "karma-es6-shim": "^1.0.0",
    "karma-firefox-launcher": "^1.0.0",
    "karma-mocha": "^1.1.1",
    "karma-mocha-reporter": "^2.0.4",
    "karma-phantomjs-launcher": "^1.0.1",
    "karma-remap-istanbul": "^0.1.1",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.7.0",
    "lite-server": "^2.2.2",
    "mocha": "^2.5.3",
    "phantomjs-prebuilt": "^2.1.7",
    "react-addons-css-transition-group": "^15.2.1",
    "react-addons-test-utils": "^15.2.1",
    "react-notifications": "^1.2.0",
    "sinon": "^1.17.4",
    "sinon-chai": "^2.8.0",
    "source-map-loader": "^0.1.5",
    "style-loader": "^0.13.1",
    "ts-loader": "^0.8.2",
    "typescript": "^1.8.10",
    "typings": "^1.3.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1"
  }
}
