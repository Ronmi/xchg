{
  "name": "ui",
  "version": "1.0.0",
  "description": "ui for xchg",
  "main": "js/bundle.js",
  "scripts": {
    "postinstall": "typings install",
    "typings": "typings",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "webpack": "webpack",
    "webpack:w": "webpack -w",
    "lite": "browser-sync start --config bs-config.js",
    "start": "tsc && webpack && concurrently \"npm run tsc:w\" \"npm run webpack:w\" \"npm run lite\"",
    "karma": "karma start --browsers PhantomJS,Firefox,Chrome",
    "test": "tsc && mocha build/test/**/*.js"
  },
